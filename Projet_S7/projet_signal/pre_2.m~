%% GARCIA GODOY

clear all; close all;clc;

%% Initialisation des variables

load('fcno03fz.mat');

s = fcno03fz';
RSB = 5;
N = length(s);

%% Ajout du bruit

[ s_bruit ] = bruit_rsb( s,RSB, N);

%% Affichage

figure,
subplot(2,1,1)
plot(s)
title('Représentation temporelle du signal')

subplot(2,1,2)
plot(spectrogram(s))
title('Spectrogramme du signal')

figure,
subplot(2,1,1)
plot(s)
title('Représentation temporelle du signal bruité')

subplot(2,1,2)
plot(spectrogram(s))
title('Spectrogramme du signal bru')